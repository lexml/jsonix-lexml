            <Inciso xlink:href="art13_cpt_inc1" id="art1_cpt_alt1_art13_cpt_inc1">
            
            
            <Rotulo>I –</Rotulo>
            <p>alimentação, nutrição e saúde bucal;</p>
            
          </Inciso>
          </Caput><Omissis id="art1_cpt_alt1_art13_omi1" fechaAspas="s" notaAlteracao="NR"/>
          </Artigo><Artigo xlink:href="art16" id="art1_cpt_alt1_art16" abreAspas="s">
            
            
            <Rotulo>Art. 16.</Rotulo>
            
            <Caput xlink:href="art16_cpt" id="art1_cpt_alt1_art16_cpt" textoOmitido="s">
            
            
            
            
            <Inciso xlink:href="art16_cpt_inc1" id="art1_cpt_alt1_art16_cpt_inc1">
            
            
            <Rotulo>I –</Rotulo>
            <p>formular, avaliar e apoiar políticas de saúde bucal, de alimentação e de nutrição;</p>
            
          </Inciso>
          </Caput><Omissis id="art1_cpt_alt1_art16_omi1" fechaAspas="s" notaAlteracao="NR"/>
