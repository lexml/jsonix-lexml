            <Alteracao xml:base="urn:lex:br:federal:constituicao:1988-10-05;1988" id="art3_cpt_alt1">
              <!--Matches: MatchResult(Map(urn:lex:br:federal:constituicao:1988-10-05;1988 -> MatchByBase(urn:lex:br:federal:constituicao:1988-10-05;1988,1988,Some((10,5)),Map(art57 -> ExactMatch(URN(1988,Some((10,5)),art57,urn:lex:br:federal:constituicao:1988-10-05;1988!art57)), art57_cpt -> PrefixMatch(URN(1988,Some((10,5)),art57,urn:lex:br:federal:constituicao:1988-10-05;1988!art57),art57_cpt), art57_par5 -> PrefixMatch(URN(1988,Some((10,5)),art57,urn:lex:br:federal:constituicao:1988-10-05;1988!art57),art57_par5)))))-->
