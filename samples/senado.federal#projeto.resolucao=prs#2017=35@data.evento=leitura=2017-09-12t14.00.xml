27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art357_cpt), art357 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art357), art412_par1 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art412_par1), art99_par6 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art99_par6), art356_par1 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art356_par1), art163_cpt -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art163_cpt), art163_par8 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art163_par8), art118_par4 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art118_par4), art99 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art99), art356 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art356), art125 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art125), art167_par1_inc1 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art167_par1_inc1), art356_par2 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art356_par2), art358_par3 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art358_par3), art215 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art215), art412_par2 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art412_par2), art215_cpt_inc2 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art215_cpt_inc2), art125_cpt -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art125_cpt), art140_par3 -> PrefixMatch(URN(1970,Some((11,27)),,urn:lex:br:federal:resolucao:1970-11-27;93),art140_par3), art163 -> Prefi